<% provide(:title, "Skillsets Manager") %>
<h1>Skillsets Manager</h1>

<div class="row">
  <div class="breadcrumb-layout">
    <ul class="breadcrumb">
      <li>
        <%= link_to "New", new_skillset_path %>
      </li>
    </ul>
  </div>
</div>

<div class="row">
  <div class="content-layout">

    <% if @skillsets.any? %>
      <h3>Skillsets</h3>
      <%= render "skillsets_table" %>
    <% end %>

  </div>
</div>
