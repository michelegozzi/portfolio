<% provide(:title, "Edit Contact Profile") %>

<div class="row">
  <div class="breadcrumb-layout">
    <ul class="breadcrumb">
      <li>
        <%= link_to "Back", :back %>
        <span class="divider"></span>
      </li>
      <li>
        <%= link_to "New", new_contact_profile_path %>
        <span class="divider"></span>
      </li>
      <li>
        <%= link_to "Delete", @contact_profile, method: :delete, data: { confirm: "Are you sure?" } %>
      </li>
    </ul>
  </div>
</div>

<%= form_for @contact_profile do |f| %>
  <%= render 'fields', f: f %>
<% end %>